# SOF test file - definition and retrieval tests

# basic def and retrieval
"3" x def
x . "3" = assert

# rebind possible
"hello" x def
x . "hello" = assert
# not old value
x . "3" /= assert

# bind other value
"a string" blah def
# sanity-check
blah . x . /= assert
blah . "a string" = assert

{
	# inside codeblocks, scope does not change
	x . "hello" = assert
} .
